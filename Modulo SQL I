/*Ejercicio 3*/
SELECT nombre, apellido, fecha_nacimiento FROM profesor ORDER BY fecha_nacimiento;
SELECT * from profesor WHERE salario >= 65000;
SELECT * from profesor WHERE fecha_nacimiento BETWEEN '1980' and '1990';
SELECT TOP (5) [id],[nombre],[apellido],[fecha_nacimiento],[salario] from profesor;
SELECT * from profesor WHERE apellido LIKE 'P%';
SELECT * from profesor WHERE fecha_nacimiento BETWEEN '1980' and '1990' AND salario > 80000;